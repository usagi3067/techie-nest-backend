            <result property="phone" column="phone" jdbcType="VARCHAR"/>
            <result property="validDays" column="valid_days" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,is_free,original_price,
        price,qq,wechat,
        phone,valid_days
    </sql>
</mapper>
