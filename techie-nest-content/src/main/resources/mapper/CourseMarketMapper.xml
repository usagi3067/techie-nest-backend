<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dango.content.mapper.CourseMarketMapper">

    <resultMap id="BaseResultMap" type="com.dango.content.model.entity.CourseMarket">
